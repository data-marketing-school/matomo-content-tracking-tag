// Enter your template code here.
const log = require('logToConsole');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');

function isMatomoSet() {
  const _paq = copyFromWindow("_paq");
  if (_paq) {
    return true;
  }
  log("Error: _paq variable not set, \"Matomo Analytics - Content Tracking\" tag should only be triggered after \"Matomo Analytics - Configuration\" tag.");
  data.gtmOnFailure();
  return false;
}

if(isMatomoSet()) {
  if(data.selectedContentTracking == "impression") {
    callInWindow("_paq.push", ["trackContentImpression", data.impContentName, data.impContentPiece, data.impContentTarget]);
  }

  if(data.selectedContentTracking == "interaction") {
    callInWindow("_paq.push", ["trackContentInteraction", data.intContentInteraction, data.intContentName, data.intContentPiece, data.intContentTarget]);
  }
}



// Call data.gtmOnSuccess when the tag is finished.
data.gtmOnSuccess();


